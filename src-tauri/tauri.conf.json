{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "keep-accounts",
  "version": "0.0.1",
  "identifier": "com.keep-accounts.app",
  "build": {
    "beforeDevCommand": "trunk serve",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "trunk build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "keep-accounts",
        "width": 400,
        "height": 800,
        "minWidth": 360,
        "minHeight": 640,
        "maxWidth": 600,
        "maxHeight": 1400,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}